            <Form.Control \r\n                                                                        as='select' \r\n                                                                        value={qty} \r\n                                                                        onChange={(e) => setQty(Number(e.target.value))}\r\n                                                                        style={{\r\n                                                                            width: \"80px\",\r\n                                                                            borderRadius: '50px',\r\n                                                                            padding: '6px 12px',\r\n                                                                            backgroundColor: '#f8f9fa',\r\n                                                                            border: '1px solid #ced4da',\r\n                                                                        }}\r\n                                                                    >\r\n                                                                        {[...Array(productLendMachines.quantity).keys()]\r\n                                                                            .map(x =>\r\n                                                                                <option key={x + 1} value={x + 1}>\r\n                                                                                    {x + 1}\r\n                                                                                </option>\r\n                                                                            )\r\n                                                                        }\r\n                                                                    </Form.Control>\r\n                                                                </Col>\r\n                                                            </Row>\r\n               